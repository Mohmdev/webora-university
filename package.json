  "scripts": {
    "ci": "bun i --frozen-lockfile --production",
    "update-deps": "rm node_modules bun.lock -rf && bun i",
    "update-yarn": "rm yarn.lock -rf && yarn import",
    "dev": "next dev",
    "build": "prisma generate --no-engine && next build",
    "start": "next start",
    "format": "prettier --write .",
    "prepare": "husky",
    "lint": "biome lint ./src",
    "lint:next": "next lint",
    "lint:fix": "biome lint --write ./src",
    "format:src": "biome format --write ./src",
    "fix:ci": "biome ci --formatter-enabled=true --linter-enabled=true --organize-imports-enabled=true --assists-enabled=true",
    "db:push": "prisma db push",
    "db:generate": "prisma generate --no-engine",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "db:tunnel": "bunx @prisma/ppg-tunnel"
  },
